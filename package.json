{
  "name": "@neuronet.io/vido",
  "version": "3.0.4",
  "description": "Compilation-less and eval free frontend framework for fast scalable apps.",
  "main": "dist/vido.umd.min.js",
  "module": "dist/vido.esm.min.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.global.json && tsc -p ./tsconfig.types.json && tsc -p ./tsconfig.json && rollup -c",
    "types": "tsc -p ./tsconfig.global.json && tsc -p ./tsconfig.types.json",
    "autobuild": "rollup -c -w",
    "test": "nightwatch --env chrome && nightwatch --env firefox"
  },
  "types": "types/global.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neuronetio/vido.git"
  },
  "keywords": [
    "frontend",
    "framework",
    "svelte",
    "fast",
    "compilation",
    "compilation-less",
    "functional",
    "easy"
  ],
  "author": "Rafal Pospiech <neuronet.io@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neuronetio/vido/issues"
  },
  "homepage": "https://github.com/neuronetio/vido#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "chromedriver": "^81.0.0",
    "expect": "^24.9.0",
    "geckodriver": "^1.19.1",
    "nightwatch": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "selenium-server": "^3.141.59",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "csstype": "^2.6.10",
    "lit-html-optimised": "^1.1.7"
  }
}
